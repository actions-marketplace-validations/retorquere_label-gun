{
  "name": "label-gun",
  "version": "1.0.8",
  "private": true,
  "description": "automatic issue labeler",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format-check": "prettier --check **/*.ts",
    "lint": "echo eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "preversion": "npm start && git add -u",
    "postversion": "git push --follow-tags",
    "test": "echo jest",
    "start": "npm run build && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/request-error": "^2.1.0",
    "@octokit/webhooks": "^9.7.0",
    "@octokit/webhooks-definitions": "^3.65.6",
    "fast-safe-stringify": "^2.0.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/parser": "^4.26.1",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.28.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.4",
    "jest-circus": "^27.0.4",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  }
}
